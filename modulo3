var factorial = 1;
for (var i = 1; i <= 10; i++) 
{
    factorial *= i;
}
console.log(factorial)
var factorial_for = factorial;

var i = 1;
var factorial_while = 1;
while (i <= 10) 
{
    factorial_while *= i;
    i++;
}
console.log(factorial_while)
var boolean = true;
var factorial_break = 1;
i = 1;
while (boolean) 
{
    factorial_break *= i;
    i++;
    if (i == 11) 
    {
       break;
    }
}
console.log(factorial_break)
